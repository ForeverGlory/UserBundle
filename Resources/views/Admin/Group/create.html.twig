{% extends "GloryUserBundle:Admin:layout.html.twig" %}

{% block content %}
    <div class="box">
        <div class="box-body">
            <form action="{{ path('glory_user_group_create') }}" {{ form_enctype(form) }} method="POST" class="group-create">
                {{ form_widget(form) }}
                <button type="submit" class="btn btn-default">{{ 'group.create.submit'|trans }}</button>
            </form>
        </div>
    </div>
{% endblock %}